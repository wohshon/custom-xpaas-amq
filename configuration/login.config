  LdapConfiguration {
    org.apache.activemq.jaas.LDAPLoginModule required
    debug=true
    initialContextFactory=com.sun.jndi.ldap.LdapCtxFactory
    connectionURL="ldap://192.168.223.80:389"
    connectionUsername="cn=root,dc=demo,dc=com"
    connectionPassword=redhat
    connectionProtocol=""
    authentication=simple
    userBase="ou=User,ou=messaging,ou=groups,dc=demo,dc=com"
    userRoleName=dummyUserRoleName
    userSearchMatching="(uid={0})"
    userSearchSubtree=false
    roleBase="ou=Group,ou=messaging,ou=groups,dc=demo,dc=com"
    roleName=cn roleSearchMatching="(member:=uid={1})" roleSearchSubtree=true ;
    };         
